<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">


<scenario name="Basic Sip-phone UAC">
<nop>
	<!-- rtcp_port += field0 -->
	<action>
		<assignstr assign_to="1" value="[field0 file={{ports_file}}]"/>
		<todouble assign_to="2" variable="1"/>
		<add assign_to="2" value="1"/>
		<assignstr assign_to="3" value="[$2]"/>
		<ereg regexp="[0-9]+" search_in="var" variable="3" assign_to="rtcp_port" check_it="true" />
	</action>
</nop>

  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->
  <send>
    <![CDATA[

      INVITE sip:[service]@[remote_ip]:[remote_port];transport=[transport];user=phone SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field0 file={{dn_file}}] <sip:[field0 file={{dn_file}}]@[local_ip]:[local_port];user=phone>;tag=[pid]SIPpTag00[call_number]
      To: [service] <sip:[service]@[remote_ip]:[remote_port];user=phone>
      User-Agent: LbV caller
      Call-ID: [call_id]
      CSeq: 1 INVITE
      Contact: "[field0 file={{dn_file}}]" <sip:[field0 file={{dn_file}}]@[local_ip]:[local_port];transport=[transport]>
      Max-Forwards: 70
      Allow: INVITE,ACK,CANCEL,BYE,REFER,NOTIFY,MESSAGE,UPDATE
      Allow-Events: talk, hold, conference
      P-Asserted-Identity: "[field0 file={{dn_file}}]" <sip:[field0 file={{dn_file}}]@[remote_ip]:[remote_port];user=phone>
      Supported: replaces, 100rel
      X-Siemens-Call-Type: ST-insecure
      Geolocation: <cid:[field0 file={{dn_file}}]@[local_ip]>
      Content-Type: multipart/mixed;boundary=level-0
      Subject: Performance Test
      Content-Length: [len]

      --level-0
      Content-Type: application/sdp

      v=8
      o=MxSIP 0 1739659827218 IN IP[local_ip_type] {{media_ip}}
      s=SIP Call
      c=IN IP[local_ip_type] {{media_ip}}
      t=0 0
      m=audio [field0 file={{ports_file}}] RTP/AVP 8
      a=rtpmap:8 PCMA/8000
      a=ssrc:2516680834 cname:[service]@[remote_ip]
      a=ptime:20
      a=rtcp:[$rtcp_port] IN[local_ip_type] {{media_ip}}

      --level-0
      Content-Type: application/pidf+xml
      Content-ID: <[field0]@[local_ip]>

      <?xml version="1.0" encoding="UTF-8"?><presence xmlns="urn:ietf:params:xml:ns:pidf" xmlns:gp="urn:ietf:params:xml:ns:pidf:geopriv10" xmlns:gbp="urn:ietf:params:xml:ns:pidf:geopriv10:basicPolicy" xmlns:ca="urn:ietf:params:xml:ns:pidf:geopriv10:civicAddr" entity="sip:[field0]@[local_ip]"><tuple id="SCS2-[local_ip]:[local_port]"><status>
      <gp:geopriv>
      <gp:location-info>
      <ca:civicAddress xmlns:ca="urn:ietf:params:xml:ns:pidf:geopriv10:civicAddr">
      <ca:country>GR</ca:country>
      <ca:A1>ATH</ca:A1>
      <ca:A2>Amarousion</ca:A2>
      <ca:A3>Athens</ca:A3>
      <ca:RD>Perikleous</ca:RD>
      </ca:civicAddress>
      </gp:location-info>
      <gp:usage-rules></gp:usage-rules>
      <gp:method>LLDP-MED</gp:method>
      </gp:geopriv>
      <provided-by xmlns="urn:ietf:params:xml:ns:EmergencyCallData">
      <EmergencyCallDataValue>
      <EmergencyCallData.ProviderInfo xmlns="urn:ietf:params:xml:ns:EmergencyCallData:ProviderInfo">
      <DataProviderReference>2b87b073-5598-4ede-9c5b-05d0147ce891</DataProviderReference>
      <ProviderID>urn:nena:companyid:ATT</ProviderID>
      <ProviderIDSeries>NENA</ProviderIDSeries>
      <TypeOfProvider>Telecom Provider</TypeOfProvider>
      </EmergencyCallData.ProviderInfo>
      <EmergencyCallData.ServiceInfo xmlns="urn:ietf:params:xml:ns:EmergencyCallData:ServiceInfo">
      <DataProviderReference>2b87b073-5598-4ede-9c5b-05d0147ce891</DataProviderReference>
      <ServiceEnvironment>Residence</ServiceEnvironment>
      <ServiceType>POTS</ServiceType>
      <ServiceMobility>Fixed</ServiceMobility>
      <LegacyClassOfService type="1" xmlns="http://ddti.net/addcallinfo">RESD</LegacyClassOfService>
      </EmergencyCallData.ServiceInfo>
      <EmergencyCallData.DeviceInfo xmlns="urn:ietf:params:xml:ns:EmergencyCallData:DeviceInfo">
      <DataProviderReference>2b87b073-5598-4ede-9c5b-05d0147ce891</DataProviderReference>
      </EmergencyCallData.DeviceInfo>
      <EmergencyCallData.SubscriberInfo privacyRequested="false" xmlns="urn:ietf:params:xml:ns:EmergencyCallData:SubscriberInfo">
      <DataProviderReference>2b87b073-5598-4ede-9c5b-05d0147ce891</DataProviderReference>
      <SubscriberData>
      <vcard xmlns="urn:ietf:params:xml:ns:vcard-4.0">
      <geo>
      <uri>geo:37.66528,-121.8792</uri>
      </geo>
      <tel>
      <uri>tel:9255111002</uri>
      </tel>
      </vcard>
      </SubscriberData>
      </EmergencyCallData.SubscriberInfo>
      <EmergencyCallData.Comment xmlns="urn:ietf:params:xml:ns:EmergencyCallData:Comment">
      <DataProviderReference>2b87b073-5598-4ede-9c5b-05d0147ce891</DataProviderReference>
      </EmergencyCallData.Comment>
      </EmergencyCallDataValue>
      </provided-by>
      </status>
      <timestamp>2025-02-15T22:50:26+00:00</timestamp></tuple></presence>
      --level-0--

    ]]>
  </send>

  <recv response="100" optional="true">
  </recv>

  <recv response="180" optional="true">
  </recv>

  <!-- By adding rrs="true" (Record Route Sets), the route sets         -->
  <!-- are saved and used for following messages sent. Useful to test   -->
  <!-- against stateful SIP proxies/B2BUAs.                             -->

  <recv response="200" rtd="true" crlf="true">
      <!-- assigne to [$daddress] the responded media IP address -->
     <action>
	  <ereg search_in="body"
		  check_it="true"
		  regexp="m=audio ([0-9]+) "
		  assign_to="dummy,dport"/>
	  <ereg search_in="body"
		  check_it="true"
		  regexp="c=IN IP4 ([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3})"
		  assign_to="dummy,daddress"/>

          <!-- Get the received Contact: -->
	  <ereg search_in="hdr"
		  regexp="&lt;([^&gt;]*)"
		  header="Contact:"
		  check_it="true"
		  assign_to="dummy,received-contact" />
    </action>
  </recv>
  <Reference variables="dummy"/>

  <!-- Packet lost can be simulated in any send/recv message by         -->
  <!-- by adding the 'lost = "10"'. Value can be [1-100] percent.       -->
  <send>
    <![CDATA[

      ACK [$received-contact] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [field0 file={{dn_file}}] <sip:[field0 file={{dn_file}}]@[local_ip]:[local_port]>;tag=[pid]SIPpTag00[call_number]
      To: [service] <sip:[service]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 1 ACK
      Contact: "[field0 file={{dn_file}}]" sip:[field0 file={{dn_file}}]@[local_ip]:[local_port];transport=[local_ip_type]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>

  <!-- start media streaming (RTP & RTCP) -->
  <nop>
     <action>
	     <exec command="curl -s 'http://{{media_server}}/stream?port=[field0 file={{ports_file}}]&daddress=[$daddress]&dport=[$dport]&duration={{duration}}&client'" />
     </action>
  </nop>

  <!-- Pause x seconds, which is approximately the duration of the  call -->
  <pause milliseconds="{{duration}}"/>


  <!-- The 'crlf' option inserts a blank line in the statistics report. -->
  <send>
    <![CDATA[

      BYE [$received-contact] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      Max-Forwards: 70
      From: "[field0 file={{dn_file}}]" <sip:[field0 file={{dn_file}}]@[local_ip]:[local_port]>;tag=[pid]SIPpTag00[call_number]
      To: <sip:[service]@[remote_ip]:[remote_port];user=phone>[peer_tag_param]
      Call-ID: [call_id]
      X-Siemens-Call-Type: ST-insecure
      CSeq: 2 BYE
      Supported: timer
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" crlf="true">
  </recv>


  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10, 50, 100, 500, 1000, 5000, 10000"/>

</scenario>

